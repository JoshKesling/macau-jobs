.row
  .col-sm-10.col-sm-offset-1
    .panel.panel-primary
      .panel-heading
        %h3= @agent.name
      .panel-body
        %h4
          %b Email:
        %p
          = @agent.email
        %hr
        %h4 
          %b Address:
        %p
          = simple_format(@agent.address)
        %hr
        %h4
          %b Phone:
        %p
          = @agent.phone
